{
    "name": "react-guessing-game",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
      "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
      "start:prod": "node server.js",
      "start:dev": "concurrently \"nodemon --ignore 'memory-game/*'\" \"npm run memory-game\"",
      "client": "cd memory-game && npm run start",
      "seed": "node scripts/seedDB.js",
      "install": "cd memory-game && npm install",
      "build": "cd memory-game && npm run build",
      "heroku-postbuild": "npm run build"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/keithbooher/React-Guessing-Game.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/keithbooher/React-Guessing-Game/issues"
    },
    "devDependencies": {
      "concurrently": "^3.5.0",
      "nodemon": "^1.11.0"
    },
    "homepage": "https://github.com/keithbooher/React-Guessing-Game#readme",
    "dependencies": {
      "body-parser": "^1.18.3",
      "express": "^4.16.3"
    }
  }
  
  
  
